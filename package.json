  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.8",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  }
}
